{{#each plants}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{commonName}}
    </h2>
    <a href="/plant/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fa-solid fa-seedling"></i>
      </span>
    </a>
    <a href="/plant/{{_id}}/delete" class="button">
       <span class="icon is-small">
        <i class="fa-solid fa-trash"></i>
       </span>
    </a>
  </div>
{{/each}}